
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "salary": {
      "type": "integer",
      "minimum": 0
    },
    "equity": {
      "type": "number",
      "minimum": 0,
      "maximum": 1.0
    },
    "company_handle": {
      "type": "string",
      "maxLength": 25,
      "minLength": 1
    }
  },
  "required": ["title", "salary", "company_handle"]
}
